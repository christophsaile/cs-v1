{{!-- 
Heading options:
	type: string 
		h1, h2, h3, h4, h5
	has-style: boolean
	no-margin: boolen
--}}

<div style="padding: 0 6%;">
	<h1 style="color: black; background: white">Normal Headings</h1>
	<div style="margin: 2rem 0">
		<heading-comp type="h1" data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Test Heading h1
		</heading-comp>
		<heading-comp type="h2" data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Test Heading h2
		</heading-comp>
		<heading-comp type="h3" data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Test Heading h3
		</heading-comp>
		<heading-comp type="h4" data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Test Heading h4
		</heading-comp>
		<heading-comp type="h5" data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Test Heading h5
		</heading-comp>
	</div>
	<h1 style="color: black; background: white">Styled Headings</h1>
	<div style="margin: 2rem 0">
		<heading-comp type="h1" has-style data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Styled Heading h1
		</heading-comp>
		<br>
		<heading-comp type="h2" has-style data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Styled Heading h2
		</heading-comp>
		<br>
		<heading-comp type="h3" has-style data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Styled Heading h3
		</heading-comp>
		<br>
		<heading-comp type="h4" has-style data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Styled Heading h4
		</heading-comp>
		<br>
		<heading-comp type="h5" has-style data-resources="[{paths: ['components/HeadingComp/index.js']}]">
			Styled Heading h5
		</heading-comp>
	</div>
</div>